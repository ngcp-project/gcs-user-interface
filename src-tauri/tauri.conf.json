{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:4000"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Interface",
  "mainBinaryName": "Interface",
  "version": "0.0.0",
  "identifier": "com.gcs-user-interface.app",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Camera Screen",
        "width": 800,
        "height": 600
      },
      {
        "label": "StaticScreen",
        "title": "Over View",
        "url": "http://localhost:4000/#/StaticScreen"
      }
    ],
    "security": {
      "csp": null
    }
  }
}
